  "name": "@layercode/react-sdk",
  "version": "2.3.0",
  "description": "Layercode React SDK for integrating with React applications",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
